class Solution {
public:
    vector<int> runningSum(vector<int>& nums) {
        
        for(int i=1; i<nums.size(); i++){
            
            nums[i]+= nums[i-1];
        }
        return nums;
    }
};


//solution 2:
// creates another array and stores the value there
class Solution {
public:
    vector<int> runningSum(vector<int>& nums) {
        
        vector<int> j;
        int sum=0;
        
        for(int i=0; i<nums.size(); i++){
            sum+= nums[i];
            j.push_back(sum);
                
        }
        return j;
    }
};
